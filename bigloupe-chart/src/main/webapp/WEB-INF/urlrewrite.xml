<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>

    <!--  For Graphite/Carbon servlet -->
    <rule>
        <from>^/render?(.*)$</from>
        <to type="forward">/graphite/render.json?$1</to>
    </rule>


</urlrewrite>

